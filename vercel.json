{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "prisma/schema.prisma",
            "use": "@prisma/cli",
            "config": { "prisma-version": "2.0.0" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}
